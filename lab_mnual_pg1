#include<stdio.h>
int main(){
    int n,val;
    printf("enter the size of the array");
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        printf("enter the element");
        scanf("%d",&arr[i]);
    }
    printf("enter the element to be searched in the array: ");
    scanf("%d",&val);
    for(int i=0;i<n;i++){
        if(val==arr[i]){
            printf("element found ");
            break;
        }
        else{
            printf("element not found ");
            break;
        }
    }
    return 0;
}
